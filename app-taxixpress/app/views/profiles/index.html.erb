<p id="notice"><%= notice %></p>

<h1>Listing Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Nombre empresa</th>
      <th>Ruc</th>
      <th>Direccion</th>
      <th>Email</th>
      <th>Contraseña</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.nombre %></td>
        <td><%= profile.apellido %></td>
        <td><%= profile.nombre_empresa %></td>
        <td><%= profile.ruc %></td>
        <td><%= profile.direccion %></td>
        <td><%= profile.email %></td>
        <td><%= profile.contraseña %></td>
        <td><%= profile.user %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile', new_profile_path %>
